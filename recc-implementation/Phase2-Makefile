phase2: recc-implementation/bootstrap_phase_2
	@$(VALGRIND) recc-implementation/bootstrap_phase_2
	@echo "Completed phase 2 of bootstrapping process."
	@make -e -s $(DEFERRED_GLOALS)

recc-implementation/compiler_interface_common.o: recc-implementation/compiler_interface_common.c recc-implementation/compiler_interface_common.h
	@$(HOSTCC) -c recc-implementation/compiler_interface_common.c -o recc-implementation/compiler_interface_common.o $(CUSTOM_C89_FLAGS)

recc-implementation/compiler_interface_header_targets.o: recc-implementation/compiler_interface_header_targets.c recc-implementation/compiler_interface_header_targets.h
	@$(HOSTCC) -c recc-implementation/compiler_interface_header_targets.c -o recc-implementation/compiler_interface_header_targets.o $(CUSTOM_C89_FLAGS)

recc-implementation/compiler_interface_phase_2.o: recc-implementation/compiler_interface_phase_2.c recc-implementation/compiler_interface_phase_2.h
	@$(HOSTCC) -c recc-implementation/compiler_interface_phase_2.c -o recc-implementation/compiler_interface_phase_2.o $(CUSTOM_C89_FLAGS)

recc-implementation/bootstrap_phase_2.o: recc-implementation/compiler_interface_phase_2.h
	@$(HOSTCC) -c recc-implementation/bootstrap_phase_2.c -o recc-implementation/bootstrap_phase_2.o $(CUSTOM_C89_FLAGS)

$(CROSS_BUILD_PREFIX)recc-implementation/phase2_recc.o: $(CROSS_BUILD_PREFIX)recc-implementation/phase2_recc.c
	@$(HOSTCC) -c $(CROSS_BUILD_PREFIX)recc-implementation/phase2_recc.c -o $(CROSS_BUILD_PREFIX)recc-implementation/phase2_recc.o $(CUSTOM_C89_FLAGS)

#  This separate rule is used when we need to differentiate between the copes of the bootstrapping process in cross builds when the prefix is set.
recc-implementation/non_cross_phase2_recc.o: recc-implementation/phase2_recc.c
	@$(HOSTCC) -c recc-implementation/phase2_recc.c -o recc-implementation/non_cross_phase2_recc.o $(CUSTOM_C89_FLAGS)

recc-implementation/bootstrap_phase_2: recc-implementation/compiler_interface_common.o recc-implementation/compiler_interface_header_targets.o recc-implementation/non_cross_phase2_recc.o recc-implementation/compiler_interface_phase_2.o recc-implementation/bootstrap_phase_2.o recc-implementation/libbootstrap.o recc-implementation/binary_exponential_buffer.o recc-implementation/replace_tool.o generated/unsigned_char_ptr_list.o generated/unsigned_char_ptr_to_struct_entity_ptr_map.o generated/unsigned_char_ptr_to_unsigned_char_ptr_map.o generated/unsigned_char_list.o generated/struct_unsigned_char_ptr_to_unsigned_char_ptr_key_value_pair_compare.o generated/struct_unsigned_char_ptr_to_struct_entity_ptr_key_value_pair_compare.o generated/struct_unsigned_char_ptr_to_unsigned_char_ptr_key_value_pair_binary_search.o generated/struct_unsigned_char_ptr_to_struct_entity_ptr_key_value_pair_binary_search.o generated/struct_entity_ptr_list.o generated/struct_struct_entity_relationship_to_struct_struct_entity_ptr_list_ptr_key_value_pair_compare.o generated/struct_entity_relationship_to_struct_struct_entity_ptr_list_ptr_map.o generated/struct_struct_entity_ptr_list_ptr_list.o generated/struct_entity_relationship_list.o generated/struct_struct_entity_relationship_to_struct_struct_entity_ptr_list_ptr_key_value_pair_binary_search.o
	@$(HOSTCC) recc-implementation/compiler_interface_common.o recc-implementation/compiler_interface_header_targets.o recc-implementation/non_cross_phase2_recc.o recc-implementation/compiler_interface_phase_2.o recc-implementation/bootstrap_phase_2.o recc-implementation/libbootstrap.o recc-implementation/binary_exponential_buffer.o recc-implementation/replace_tool.o generated/unsigned_char_ptr_list.o generated/unsigned_char_ptr_to_unsigned_char_ptr_map.o generated/unsigned_char_ptr_to_struct_entity_ptr_map.o generated/unsigned_char_list.o generated/struct_unsigned_char_ptr_to_unsigned_char_ptr_key_value_pair_compare.o generated/struct_unsigned_char_ptr_to_struct_entity_ptr_key_value_pair_compare.o generated/struct_unsigned_char_ptr_to_unsigned_char_ptr_key_value_pair_binary_search.o generated/struct_unsigned_char_ptr_to_struct_entity_ptr_key_value_pair_binary_search.o generated/struct_entity_ptr_list.o generated/struct_struct_entity_relationship_to_struct_struct_entity_ptr_list_ptr_key_value_pair_compare.o generated/struct_entity_relationship_to_struct_struct_entity_ptr_list_ptr_map.o generated/struct_struct_entity_ptr_list_ptr_list.o generated/struct_entity_relationship_list.o generated/struct_struct_entity_relationship_to_struct_struct_entity_ptr_list_ptr_key_value_pair_binary_search.o -o recc-implementation/bootstrap_phase_2 $(CUSTOM_C89_FLAGS)

clean-phase-2: clean-phase-1
	@rm -f recc-implementation/bootstrap_phase_2
