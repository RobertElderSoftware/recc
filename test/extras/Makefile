TESTING_EXTRAS_SOURCES=$(wildcard test/extras/*.c)

TESTING_EXTRAS_OBJECTS=$(TESTING_EXTRAS_SOURCES:.c=.o)
TESTING_EXTRAS_PREPROCESSED=$(TESTING_EXTRAS_SOURCES:.c=.i)
TESTING_EXTRAS_L2=$(TESTING_EXTRAS_SOURCES:.c=.l2)

test/extras/%.o: test/extras/%.c
	@$(HOSTCC) -c $< -o $@ $(CUSTOM_C89_FLAGS)

clean-test-extras:
	@rm -f $(TESTING_EXTRAS_OBJECTS) $(TESTING_EXTRAS_PREPROCESSED) $(TESTING_EXTRAS_L2)
